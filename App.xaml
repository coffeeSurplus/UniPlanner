					<Setter Property="VerticalAlignment" Value="Center"/>
				</Style>
			</Style.Resources>
		</Style>

		<Style TargetType="Button" x:Key="MinimiseButton">
			<Setter Property="Background" Value="#00000000"/>
			<Setter Property="Focusable" Value="False"/>
			<Setter Property="WindowChrome.IsHitTestVisibleInChrome" Value="True"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="Button">
						<Border x:Name="Border" Background="{TemplateBinding Background}">
							<Line X1="0" X2="12" Stroke="#19223F" StrokeThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
						</Border>
						<ControlTemplate.Triggers>
							<Trigger Property="IsMouseOver" Value="True">
								<Setter Property="Background" Value="#F2F2F2" TargetName="Border"/>
							</Trigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<Style TargetType="Button" x:Key="MaximiseButton">
			<Setter Property="Background" Value="#00000000"/>
			<Setter Property="Focusable" Value="False"/>
			<Setter Property="WindowChrome.IsHitTestVisibleInChrome" Value="True"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="Button">
						<Border x:Name="Border" Background="{TemplateBinding Background}">
							<Rectangle Width="12" Height="10" Stroke="#19223F" StrokeThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
						</Border>
						<ControlTemplate.Triggers>
							<Trigger Property="IsMouseOver" Value="True">
								<Setter Property="Background" Value="#F2F2F2" TargetName="Border"/>
							</Trigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<Style TargetType="Button" x:Key="NormalButton">
			<Setter Property="Background" Value="#00000000"/>
			<Setter Property="Focusable" Value="False"/>
			<Setter Property="WindowChrome.IsHitTestVisibleInChrome" Value="True"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="Button">
						<Border x:Name="Border" Background="{TemplateBinding Background}">
							<Grid>
								<Rectangle Width="12" Height="10" Margin="2,0,0,2" Stroke="#19223F" StrokeThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
								<Rectangle Width="12" Height="10" Margin="0,2,2,0" x:Name="Icon" Fill="#E5E5E5" Stroke="#19223F" StrokeThickness="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
							</Grid>
						</Border>
						<ControlTemplate.Triggers>
							<Trigger Property="IsMouseOver" Value="True">
								<Setter Property="Background" Value="#F2F2F2" TargetName="Border"/>
								<Setter Property="Fill" Value="#F2F2F2" TargetName="Icon"/>
							</Trigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<Style TargetType="Button" x:Key="CloseButton">
			<Setter Property="Background" Value="#00000000"/>
			<Setter Property="Focusable" Value="False"/>
			<Setter Property="WindowChrome.IsHitTestVisibleInChrome" Value="True"/>
